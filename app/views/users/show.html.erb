<section class="user-show">

  <header class="page-header">
    <h1><%= @user.email %></h1>
  </header>

  <div class="panel panel-default items">
    <table class="table table-bordered">
      <thead>
        <tr>
          <th>ID</th>
          <th>Email</th>
          <th>First Name</th>
          <th>Last Name</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td>
            <%= @user.id %>
          </td>
          <td>
            <%= @user.email %>
          </td>
          <td>
            <%= @user.first_name %>
          </td>
          <td>
            <%= @user.last_name %>
          </td>
        </tr>
      </tbody>
    </table>
  </div>



</section>